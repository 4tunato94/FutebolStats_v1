    <line x1="590" y1="10" x2="590" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="630" y1="10" x2="630" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="670" y1="10" x2="670" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="710" y1="10" x2="710" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="750" y1="10" x2="750" y2="510" stroke="rgba(255,255,255,0.1)"/>
    
    <!-- Linhas horizontais -->
    <line x1="10" y1="50" x2="790" y2="50" stroke="rgba(255,255,255,0.1)"/>
    <line x1="10" y1="90" x2="790" y2="90" stroke="rgba(255,255,255,0.1)"/>
