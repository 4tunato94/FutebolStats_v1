    <line x1="210" y1="10" x2="210" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="250" y1="10" x2="250" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="290" y1="10" x2="290" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="330" y1="10" x2="330" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="370" y1="10" x2="370" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="430" y1="10" x2="430" y2="510" stroke="rgba(255,255,255,0.1)"/>
