<!-- Campo de futebol horizontal 20x13 zonas -->
<svg width="800" height="520" viewBox="0 0 800 520" xmlns="http://www.w3.org/2000/svg">
  <!-- Fundo do campo -->
  <rect width="800" height="520" fill="#4CAF50"/>
  
  <!-- Linhas do campo -->
  <g stroke="white" stroke-width="2" fill="none">
    <!-- Bordas -->
    <rect x="10" y="10" width="780" height="500"/>
    
    <!-- Linha do meio -->
    <line x1="400" y1="10" x2="400" y2="510"/>
    
    <!-- Círculo central -->
    <circle cx="400" cy="260" r="60"/>
    
    <!-- Áreas -->
    <rect x="10" y="160" width="120" height="200"/>
    <rect x="670" y="160" width="120" height="200"/>
    
    <!-- Pequenas áreas -->
    <rect x="10" y="210" width="60" height="100"/>
    <rect x="730" y="210" width="60" height="100"/>
    
    <!-- Arcos da área -->
    <path d="M 130 160 A 60 60 0 0 1 130 360"/>
    <path d="M 670 160 A 60 60 0 0 0 670 360"/>
  </g>
  
  <!-- Gols -->
  <rect x="0" y="235" width="10" height="50" fill="white"/>
  <rect x="790" y="235" width="10" height="50" fill="white"/>
  
  <!-- Zonas invisíveis 20x13 -->
  <g opacity="0">
    <!-- Linhas verticais -->
    <line x1="50" y1="10" x2="50" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="90" y1="10" x2="90" y2="510" stroke="rgba(255,255,255,0.1)"/>
    <line x1="130" y1="10" x2="130" y2="510" stroke="rgba(255,255,255,0.1)"/>
